    "for _ in range(3):\n",
    "    n = int(input())\n",
    "    li = [int(input()) for i in range(n)]\n",
    "    if sum(li) == 0:\n",
    "        print('0')\n",
    "    elif sum(li) > 0:\n",
    "        print('+')\n",
    "    else:\n",
    "        print('-')"
